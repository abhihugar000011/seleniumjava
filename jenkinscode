node {
    stage('Checkout') {
        checkout([$class: 'GitSCM',
                  branches: [[name: '*/main']],
                  userRemoteConfigs: [[
                      url: 'https://github.com/abhihugar000011/seleniumjava.git',
                      credentialsId: 'github-credentials-id'
                  ]]
        ])
    }

    stage('Compile Java') {
        sh 'javac AutomationDemoSite.java Example1.java'
    }

    stage('Run AutomationDemoSite') {
        sh 'java AutomationDemoSite'
    }

    stage('Run Example1') {
        sh 'java Example1'
    }
}
