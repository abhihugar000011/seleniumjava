node {
    stage('Checkout') {
    git branch: 'main', url: 'https://github.com/abhihugar000011/seleniumjava.git'
}

    stage('Compile Java') {
        sh 'javac AutomationDemoSite.java Example1.java'
    }

    stage('Run AutomationDemoSite') {
        sh 'java AutomationDemoSite'
    }

    stage('Run Example1') {
        sh 'java Example1'
    }
}
