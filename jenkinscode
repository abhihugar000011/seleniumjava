node {
    stage('Checkout') {
        git branch: 'main', url: 'https://github.com/abhihugar000011/seleniumjava.git'
    }

    stage('Compile Java') {
        bat 'javac AutomationDemoSite.java Example1.java'
    }

    stage('Run AutomationDemoSite') {
        bat 'java AutomationDemoSite'
    }

    stage('Run Example1') {
        bat 'java Example1'
    }
}
